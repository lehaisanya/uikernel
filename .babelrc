{
  "presets": [
    "@babel/preset-react",
    "@babel/preset-env",
    "@babel/preset-typescript"
  ],
  "plugins": [
    "@babel/plugin-transform-runtime",
    "babel-plugin-add-module-exports",

    // Stage 0
    "@babel/plugin-proposal-function-bind",

    // Stage 3
    "@babel/plugin-proposal-class-properties"
  ]
}
